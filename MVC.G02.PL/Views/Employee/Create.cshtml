@model EmployeeViewModel
@{
    ViewData["Title"] = "Create";
}
<h1>Create Employee</h1>
<br/>
<br />
<form method="post" asp-controller="Employee" asp-action="Create" enctype="multipart/form-data">
   <div asp-validation-summary="All" class="alert alert-danger"></div>

   
    <partial name="partial/EditcreateEmployeePartialView" model="Model" />
    <br/>
    <input asp-controller="Employee" type="submit"value="Create"class="btn btn-primary" />
    <a asp-action="Index" class="btn btn-light">Back To List</a>

</form>
@section Scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}