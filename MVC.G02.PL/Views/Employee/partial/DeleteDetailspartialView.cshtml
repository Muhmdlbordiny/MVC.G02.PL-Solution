@model EmployeeViewModel
<div class="form-group">
    <label asp-for="Name" class="col-form-lable"></label>
    <input asp-for="Name" class="form-control" readonly />

</div>
<div class="form-group">
    <label asp-for="Age" class="col-form-label"></label>
    <input asp-for="Age" class="form-control" readonly />
</div>
<div class="form-group">
    <label asp-for="Address" class="col-form-lable"></label>
    <input asp-for="Address" class="form-control" readonly />
</div>

<div class="form-group">
    <label asp-for="Email" class="col-form-lable"></label>
    <input asp-for="Email" class="form-control" readonly />
</div>
<div class="form-group">
    <label asp-for="Salary" class="col-form-lable"></label>
    <input asp-for="Salary" class="form-control" readonly />
</div>
<div class="form-group">
    <label asp-for="PhoneNumber" class="col-form-lable"></label>
    <input asp-for="PhoneNumber" class="form-control" readonly />
</div>
<div class="form-group">
    <label asp-for="IsActive" class="col-form-lable"></label>
    <input asp-for="IsActive" class="form-check" readonly />
</div>
<div class="form-group">
    <label asp-for="HiringDate" class="col-form-lable"></label>
    <input asp-for="HiringDate" class="form-control" readonly />
</div>
<div class="col-md-6">
    <label asp-for="ImageName" class="col-form-lable"></label>
    <img class="card-img-top mb-5 mb-md-0" src="~/files/images/@Model.ImageName" alt=".." width="300" height="300" />
    </div>

